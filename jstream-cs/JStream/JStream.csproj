<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <LangVersion>latest</LangVersion>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageId>JStream</PackageId>
    <PackageVersion>1.0.0</PackageVersion>
    <Authors>S.Fuchita (soramimi)</Authors>
    <Description>A lightweight, flexible JSON parser and generator library for C#. Port of jstream C++ library.</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryUrl>https://github.com/soramimi/jstream</RepositoryUrl>
    <PackageTags>json;parser;generator;streaming;c#</PackageTags>
  </PropertyGroup>
</Project>